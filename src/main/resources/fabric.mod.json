{
    "schemaVersion": 1,
    "id": "examplemod",
    "version": "${version}",
    "name": "ExampleMod",
    "description": "Example client mod for Fabric using Kotlin.",
    "authors": [
        "Squagward"
    ],
    "contact": {
        "homepage": "https://github.com/camnwalter/Fabric-Example-Mod",
        "sources": "https://github.com/camnwalter/Fabric-Example-Mod"
    },
    "license": "MIT",
    "icon": "assets/examplemod/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [{
            "adapter": "kotlin",
            "value": "com.example.ExampleMod"
        }]
    },
    "mixins": ["examplemod.mixins.json"],
    "depends": {
        "fabricloader": ">=0.14.21",
        "minecraft": "~1.20.1",
        "java": ">=17",
        "fabric-api": "*",
        "fabric-language-kotlin": ">=${fabric_kotlin_version}"
    }
}